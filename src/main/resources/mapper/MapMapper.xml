<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tencent.wxcloudrun.dao.RoomMapper">

    <select id="getLastMapRoom" resultType="com.tencent.wxcloudrun.model.MapRole">
        SELECT mapRoom FROM MapRole WHERE roomId=#{roomId} AND roleId=#{roleId} ORDER BY time DESC LIMIT 1
    </select>
</mapper>